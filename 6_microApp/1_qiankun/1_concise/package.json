{
    "name": "zy-micro-admin",
    "version": "1.0.0",
    "description": "qiankun micro",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git@github.com:markgzl/qiankun-micro.git"
    },
    "author": "markgzl",
    "license": "MIT",
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    },
    "scripts": {
        "install": "npm-run-all --serial install:*",
        "install:main": "cd main && yarn",
        "install:app1": "cd app1 && yarn",
        "install:app2": "cd app2 && yarn",
        "start": "npm-run-all --parallel start:*",
        "start:main": "cd main && yarn start",
        "start:app1": "cd app1 && yarn start",
        "start:app2": "cd app2 && yarn start",
        "start:app3": "cd app3 && serve",
        "start:app4": "cd app4 && nodemon koa.js"
    },
    "dependencies": {
        "serve": "^14.0.1"
    }
}
